package teste;

import br.com.fiap.healthTrack.Esporte;

/**
 * Classe de Testes para Esporte
 */
public class TesteEsporte {
	
	/**
	 * função de testes
	 * @param args Argumentos para testes
	 */
	public static void main(String[] args) {
		Esporte esporte = new Esporte(
				"corrida",
				100.00f,
				500.00f
			);
		
		System.out.println(esporte.getEsporte());
		System.out.println(esporte.getCaloriaKm());
		System.out.println(esporte.getCarbonoKm());
	}

}
